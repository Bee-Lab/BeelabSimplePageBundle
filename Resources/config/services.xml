<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="beelab_simple_page.validator.no_existing_route" class="Beelab\SimplePageBundle\Validator\Constraints\NoExistingRouteValidator">
            <argument type="service" id="router" />
            <argument>%beelab_simple_page.show_route%</argument>
            <tag name="validator.constraint_validator" alias="no_existing_route" />
        </service>
    </services>

</container>
