{
    "name": "beelab/simple-page-bundle",
    "type": "symfony-bundle",
    "description": "Managament of simple pages",
    "keywords": ["Page", "CMS"],
    "homepage": "https://github.com/Bee-Lab",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Massimiliano Arione",
            "email": "massimiliano.arione@bee-lab.net"
        }
    ],
    "require": {
        "php": ">=7",
        "doctrine/orm": "^2.5",
        "symfony/framework-bundle": "^2.8|^3.3|^4.0",
        "symfony/templating": "^2.8|^3.3|^4.0",
        "symfony/validator": "^2.8|^3.3|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Beelab\\SimplePageBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    }
}
